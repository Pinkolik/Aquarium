<Window x:Class="AquariumUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AquariumUI"
        mc:Ignorable="d"
        Title="Aquarium 2018" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Image x:Name="BackgroundImage" HorizontalAlignment="Left" VerticalAlignment="Top" Stretch="Fill" MouseLeftButtonDown="BackgroundImage_MouseLeftButtonDown"/>
    </Grid>
</Window>
